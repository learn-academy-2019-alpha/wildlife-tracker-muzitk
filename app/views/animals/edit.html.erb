<h1>Edit Animals </h1>
<%= form_for @animal do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <br />

    <%= form.label :latin_name %>
    <%= form.text_area :latin_name %>
    <br />
    
     <%= form.label :kingdom %>
    <%= form.text_area :kingdom %>
    <br/>
 
    <%= form.submit "Edit"%>
 
<% end %>